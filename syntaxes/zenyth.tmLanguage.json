{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/refs/heads/master/tmlanguage.json",
	"name": "Zenyth",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.zenyth",
				"match": "\\b(if|while|for|return|let|fun)\\b"
			},
			{
				"name": "keyword.operator.zenyth",
				"match": "\\b(+|=|-|*|/|%|+=|-=|*=|/=|%=|==|!=|>|<|>=|<=)"
			}
		]
		},
		"strings": {
			"name": "string.quoted.double.zenyth",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.zenyth",
					"match": "test"
				}
			]
		}
	},
	"scopeName": "source.zenyth"
}